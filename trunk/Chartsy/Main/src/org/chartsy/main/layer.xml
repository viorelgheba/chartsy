            </file>
        </folder>
        <!--<folder name="Window">
            <file name="org-chartsy-main-intro-WelcomeAction.instance">
                <attr name="component" methodvalue="org.chartsy.main.intro.WelcomeTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.chartsy.main.intro.Bundle#CTL_WelcomeAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>-->
