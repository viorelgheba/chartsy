            <file name="org-chartsy-main-NewChart.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.chartsy.main.Bundle"/>
                <attr name="delegate" newvalue="org.chartsy.main.NewChart"/>
                <attr name="displayName" bundlevalue="org.chartsy.main.Bundle#CTL_NewChart"/>
