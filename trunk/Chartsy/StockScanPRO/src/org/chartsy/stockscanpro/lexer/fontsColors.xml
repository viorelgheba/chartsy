<fontscolors>
    
    <fontcolor name="errors" default="error"/>
    <fontcolor name="identifier" default="identifier"/>
    <fontcolor name="keyword" foreColor="4096ee" default="keyword">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="indicator" foreColor="356aa0" default="indicator">
